<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h1 class="h3">
                    Add Video
                </h1>
            </div>
            <div class="panel-body">
                
<div class="row">
    <div class="col-md-12">
        <div class="form-group has-success">
            <p class="help-block">Your video was successfully added!</p>
        </div>
        <div class="form-horizontal">
            <div class="form-group">
                <label class="col-sm-2 control-label">Video Title</label>               
                <div class="col-sm-10">
                    <p class="form-control-static">@this.Model.Title</p>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-2 control-label">Provider</label>
                <div class="col-sm-10">
                    <p class="form-control-static">@this.Model.ProviderId</p>
                </div>
            </div>
            <div class="form-group">
                <label for="providerVideoId" class="col-sm-2 control-label">Video ID</label>
                <div class="col-sm-10">
                    <p class="form-control-static">@this.Model.ProviderVideoId</p>
                </div>
            </div>
            <div class="form-group">
                <label for="skillLevel" class="col-sm-2 control-label">Level</label>
                <div class="col-sm-10">
                    <p class="form-control-static">@this.Model.SkillLevel</p>
                </div>
            </div>
            <div class="form-group">
                <label for="danceCategory" class="col-sm-2 control-label">Category</label>
                <div class="col-sm-10">
                    <p class="form-control-static">@this.Model.DanceCategory</p>
                </div>
            </div>
            <div class="form-group">
                <label for="dancerIdList" class="col-sm-2 control-label">Dancers</label>
                <div class="col-sm-10">
                    <p class="form-control-static">@this.Model.DancerNameList</p>
                </div>
            </div>
            <div class="form-group">
                <label for="dancerIdList" class="col-sm-2 control-label">Event</label>
                <div class="col-sm-10">
                    <p class="form-control-static">@this.Model.EventName</p>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <p>
            Would you like to <a href="@this.Model.VideoUrl">view your uploaded video</a>
            or <a href="@this.Model.AddVideoUrl">add another video</a>?
        </p>
    </div>
</div>

            </div>
        </div>
    </div>
</div>